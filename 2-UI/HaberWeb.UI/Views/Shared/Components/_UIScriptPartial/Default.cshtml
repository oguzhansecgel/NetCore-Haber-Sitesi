<script>
    function getContentDetail(categoryId) {

        $("#content").empty();
        var _url = 'GetContentDetail?categoryId=' + categoryId;
        $.ajax({
            type: "GET",
            url: _url,
            success: function (result) {
                $("#content").html(result);
            },
        });
    }

</script>