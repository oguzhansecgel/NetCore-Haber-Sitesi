@model List<ResultSocialMediaDto>

<div class="trending-bar-dark hidden-xs">
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                <h3 class="trending-bar-title">Trending News</h3>              
                <h3 class="text-color-white">Trending News</h3>              
            </div>
            <div class="col-md-12 col-sm-12 col-xs-12 top-nav-social-lists text-lg-right col-lg-4 ml-lg-auto">
                <ul class="list-unstyled mt-4 mt-lg-0">
                    @foreach (var item in Model)
                    {
                        <li>
                            <a href="@item.FacebookURL">
                                <span class="social-icon">
                                    <i class="fa fa-facebook-f"></i>
                                </span>
                            </a>
                            <a href="@item.TwitterURL">
                                <span class="social-icon">
                                    <i class="fa fa-twitter"></i>
                                </span>
                            </a>
                         
                            <a href="@item.YoutubeURL">
                                <span class="social-icon">
                                    <i class="fa fa-youtube"></i>
                                </span>
                            </a>
                            <a href="@item.InstagramURL">
                                <span class="social-icon">
                                    <i class="fa fa-instagram"></i>
                                </span>
                            </a>
                        </li>
                    }
                    
                </ul>
            </div>

        </div>
    </div>
</div>
