@model List<ResultSocialMediaDto>
<header class="header-navigation d-none d-lg-block">
	<div class="container">
		<div class="row align-items-center">
			<div class="col-xs-12 col-sm-3 col-md-3">
				<!-- Logo -->
				<div class="logo">
					<a href="@Url.Action("Index","UIHomePage")">
						  <!-- Replace Logo Here -->
						<img src="~/newsbit-bootstrap-main/theme/images/logos/kanal-logo.png" alt="" width="170px" height="60px">
					</a>
				</div>
				<!-- End Logo -->
			</div>
            <div class="col-md-12 col-sm-12 col-xs-12 top-nav-social-lists text-lg-right col-lg-4 ml-lg-auto">
                <ul class="list-unstyled mt-4 mt-lg-0">
                    @foreach (var item in Model)
                    {
                        <li>
                            <a href="@item.FacebookURL">
                                <span class="social-icon">
                                    <i class="fa fa-facebook-f"></i>
                                </span>
                            </a>
                            <a href="@item.TwitterURL">
                                <span class="social-icon">
                                    <i class="fa fa-twitter"></i>
                                </span>
                            </a>

                            <a href="@item.YoutubeURL">
                                <span class="social-icon">
                                    <i class="fa fa-youtube"></i>
                                </span>
                            </a>
                            <a href="@item.InstagramURL">
                                <span class="social-icon">
                                    <i class="fa fa-instagram"></i>
                                </span>
                            </a>
                        </li>
                    }

                </ul>
            </div>
		</div>
	</div>
</header>